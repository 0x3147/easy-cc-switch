# 生产环境配置
NODE_ENV=production

# 代码保护相关
# 如果需要代码签名，请设置以下环境变量
# WIN_CSC_LINK=path/to/certificate.pfx
# WIN_CSC_KEY_PASSWORD=your_certificate_password
# MAC_CSC_LINK=path/to/certificate.p12
# MAC_CSC_KEY_PASSWORD=your_certificate_password

# 禁用 source map（生产环境）
GENERATE_SOURCEMAP=false

# 禁用调试信息
DEBUG=false
